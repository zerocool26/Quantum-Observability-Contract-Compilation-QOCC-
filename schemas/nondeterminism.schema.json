{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "QOCC Nondeterminism Report",
  "type": "object",
  "required": [
    "reproducible",
    "num_runs",
    "unique_hashes"
  ],
  "properties": {
    "reproducible": {
      "type": "boolean"
    },
    "num_runs": {
      "type": "integer"
    },
    "unique_hashes": {
      "type": "integer"
    },
    "confidence": {
      "type": "number"
    },
    "hashes": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "hash_counts": {
      "type": "object",
      "additionalProperties": {
        "type": "integer"
      }
    }
  },
  "additionalProperties": true
}

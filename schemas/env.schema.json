{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "QOCC Environment Snapshot",
  "type": "object",
  "required": [
    "os",
    "python"
  ],
  "properties": {
    "os": {
      "type": "string"
    },
    "python": {
      "type": "string"
    },
    "python_executable": {
      "type": "string"
    },
    "packages": {
      "type": "object",
      "additionalProperties": {
        "type": "string"
      }
    },
    "git_sha": {
      "type": [
        "string",
        "null"
      ]
    }
  },
  "additionalProperties": true
}
